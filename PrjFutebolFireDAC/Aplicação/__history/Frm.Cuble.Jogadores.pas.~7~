  FrmClubeJogadores: TFrmClubeJogadores;

implementation

{$R *.dfm}

uses Componentes, Frm.Pais, Frm.Principal, Frm.Tatica, Frm.Tecnicos;

procedure TFrmClubeJogadores.FormCreate(Sender: TObject);
begin
Componentes.DmComponentes.QryClube.Open;
Componentes.DmComponentes.QryTatica.Open;
Componentes.DmComponentes.QryTecnicos.Open;
Componentes.DmComponentes.QryPais.Open;
end;

procedure TFrmClubeJogadores.FormDestroy(Sender: TObject);
begin
Componentes.DmComponentes.QryClube.Close;
Componentes.DmComponentes.QryTatica.Close;
Componentes.DmComponentes.QryTecnicos.Close;
Componentes.DmComponentes.QryPais.Close;
end;

end.
