    DBLookupComboBox2: TDBLookupComboBox;
    DBGrid1: TDBGrid;
    DBNavigator2: TDBNavigator;
    DBLookupComboBox3: TDBLookupComboBox;
    Label7: TLabel;
    Label8: TLabel;
    Label9: TLabel;
    Label10: TLabel;
    Label11: TLabel;
    Label12: TLabel;
    Label13: TLabel;
    Label14: TLabel;
    Label15: TLabel;
    DsTecnico: TDataSource;
    DBEdit5: TDBEdit;
    DBEdit6: TDBEdit;
